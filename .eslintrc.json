{
  "extends": [
    "next/core-web-vitals",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended"
  ],
  "env": {
    "browser": true,
    "es2022": true,
    "node": true
    // "jest":true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "semi": [
      "warn",
      "always"
    ],
    "quotes": [
      "warn",
      "double"
    ],
    "no-undef": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}